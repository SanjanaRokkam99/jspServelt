package com.bridgeLabz.DesignPattern.Builder;

public class Vehical {
	// required parameter
	private String engine;
	private int wheel;

	// optional parameter
	private int airBag;

	public String getEngine() {
		return engine;
	}

	public int getWheel() {
		return wheel;
	}

	public int getAirBag() {
		return airBag;
	}

	private Vehical(VehicalBuilder builder)
	{
		this.engine = engine;
		this.wheel = wheel;
		this.airBag = airBag;
	}
}