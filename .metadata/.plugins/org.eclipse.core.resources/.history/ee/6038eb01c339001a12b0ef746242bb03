package com.bridgeLabz.DesignPattern.Builder;

class VehicalBuilder {

	private String engine;
	private int wheel;
	private int airBag;

	public VehicalBuilder(String engine, int wheel) {
		super();
		this.engine = engine;
		this.wheel = wheel;
	}
	public VehicalBuilder(int airBag)
	{
		this.airBag = airBag;
	}
}
